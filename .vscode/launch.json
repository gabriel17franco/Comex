{
  "inputs": [
    {
      "id": "UI5MinVersion1.65.0",
      "type": "command",
      "command": "sap.ux.appGenerator.ui5Version",
      "args": {
        "minUI5Version": "1.42.0"
      }
    }
  ],
  "configurations": [
    {
      "name": "Start zcomexapp QAS",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "cwd": "${workspaceFolder}/zcomexapp",
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "runtimeArgs": ["fiori", "run"],
      "args": [
        "--open",
        "test/flpSandbox.html#comexzcomexapp-display"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "DEBUG": "--inspect"
      }
    },
    {
      "name": "Start zcomexapp - Index QAS",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "cwd": "${workspaceFolder}/zcomexapp",
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "runtimeArgs": ["fiori", "run"],
      "args": [
        "--open",
        "index.html"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "DEBUG": "--inspect"
      }
    },
    {
      "name": "Start zcomexapp Local DEV",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/zcomexapp",
      "runtimeExecutable": "npx",
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "runtimeArgs": ["fiori", "run"],
      "args": [
        "--config",
        "./ui5-local.yaml",
        "--open",
        "test/flpSandbox.html#comexzcomexapp-display"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {}
    },
    {
      "name": "Start zcomexapp Local - Index DEV",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/zcomexapp",
      "runtimeExecutable": "npx",
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "runtimeArgs": ["fiori", "run"],
      "args": [
        "--config",
        "./ui5-local.yaml",
        "--open",
        "index.html"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {}
    }
  ],
  "compounds": []
}
