<c:FragmentDefinition
xmlns="sap.m"
xmlns:c="sap.ui.core"
xmlns:xap="sap.uxap"
xmlns:smartList="sap.ui.comp.smartlist"
xmlns:smartTable="sap.ui.comp.smarttable"
>
<xap:ObjectPageSubSection>
	<HBox>
		<smartList:SmartList entitySet="HistorySet" header="Log List"
		showRowCount="true" showFullScreenButton="true" enableAutoBinding="true" listBindingPath="GetHistory" 
	>
	<smartList:listItemTemplate>
		<StandardListItem
			title="{Stepdescription}" 
			icon="sap-icon://accept"
			iconInset="true"
			highlight="Success"
			info="{path: 'Createdon',
			type: 'sap.ui.model.type.Date',
			formatOptions: {
				style: 'short'
			}}"
			description="{Createdby}" 
			infoState="None"

		/>
			
	</smartList:listItemTemplate>
	</smartList:SmartList>
	</HBox>
</xap:ObjectPageSubSection>


</c:FragmentDefinition>